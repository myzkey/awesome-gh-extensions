{
  "name": "awesome-gh-extensions",
  "version": "1.0.0",
  "description": "Auto-updated curated list of GitHub CLI extensions",
  "type": "module",
  "scripts": {
    "generate": "bun run scripts/generate.ts",
    "typecheck": "bun run tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "github",
    "gh-extension",
    "awesome-list",
    "cli"
  ],
  "author": "myzkey",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "latest",
    "typescript": "^5.3.0",
    "vitest": "^4.0.15"
  }
}
